{
    "testEnvironment": "node",
    "transform": {},
    "extensionsToTreatAsEsm": [
        ".js"
    ],
    "moduleNameMapper": {
        "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testMatch": [
        "**/__tests__/**/*.test.js",
        "**/services/__tests__/**/*.test.js"
    ],
    "collectCoverageFrom": [
        "services/**/*.js",
        "!services/__tests__/**"
    ],
    "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/__tests__/"
    ]
}